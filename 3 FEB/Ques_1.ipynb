   "source": [
    "def odd_no_list(a,b):\n",
    "    l=[]\n",
    "    for i in range(a,b):\n",
    "        if i%2!=0:\n",
    "            l.append(i)\n",
    "    return l"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
